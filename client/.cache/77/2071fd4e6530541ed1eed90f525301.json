ovies/:movieTitle\"\n            render={({ match }) => (\n              <MovieView movie={movies.find((m) => m.title === match.params.movieTitle)} />\n            )}\n          />\n        </Switch>\n      </Router>\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"65af4a72fc79d97fe76b661944110392","cacheData":{"env":{}}}